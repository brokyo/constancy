{
	"name": "mindful",
	"version": "0.0.1",
	"description": "focus",
  "browser_action": {
    "default_popup": "popup.html"
  },
	"background": {
		"scripts": [
			"background.js"
		]
	},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "p5.min.js"],
      "run_at": "document_end"
    }
  ],
	"permissions": [
		"tabs", "storage", "declarativeContent", "alarms"
	],
	"manifest_version": 2
}